public class Person {
    //属性 field
    String name;
    int age;
    char gender;
    //方法 method
    public void eat(){
        System.out.println("Person eat");
    }
    public void sleep(int hour){
        System.out.println("Person sleep "+hour);
    }
    public void interests(String hobby){
        System.out.println(hobby);
    }


}
